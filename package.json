{
  "name": "ysyx-website",
  "version": "1.0.0",
  "description": "A project of ysyx-website",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build-local": "vuepress build src --debug",
    "build-deploy-pre": "mkdir -p var; rm -rf var/dist;",
    "build-deploy-aft": "cp -rv src/.vuepress/dist/ var/; rm -rf var/dist/res/.git",
    "build-deploy": "npm run build-deploy-pre; npm run build-local; npm run build-deploy-aft",
    "dev": "vuepress dev src --debug",
    "deploy": "node ./deploy/index.mjs",
    "clean": "vuepress dev src --clean-cache",
    "update": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/client": "2.0.0-beta.60",
    "@vuepress/plugin-register-components": "2.0.0-beta.60",
    "colors": "^1.4.0",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vp-update": "^0.2.0",
    "vue": "3.2.45",
    "vuepress": "2.0.0-beta.60",
    "vuepress-plugin-search-pro": "2.0.0-beta.166",
    "vuepress-theme-hope": "2.0.0-beta.166"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "element-plus": "^2.3.6",
    "qrcode.vue": "^3.4.0"
  },
  "deploy": "home"
}
